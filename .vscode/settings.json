{
    "[javascript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[json]": {
        "editor.defaultFormatter": "biomejs.biome",
        "editor.formatOnSave": true
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
        "editor.inlayHints.enabled": "off"
    },
    "[sql]": {
        "editor.defaultFormatter": "ReneSaarsoo.sql-formatter-vsc"
    },
    "[typescript]": {
        "editor.defaultFormatter": "biomejs.biome",
        "editor.formatOnSave": true
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "biomejs.biome",
        "editor.formatOnSave": true
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "biome_lsp.trace.server": "messages",
    "bun.debugTerminal.enabled": true,
    "bun.debugTerminal.stopOnEntry": false,
    "bun.runtime": "~/.bun/bin/bun",
    "bun.test.filePattern": "**/*{.test.,.spec.,_test_,_spec_}{js,ts,tsx,jsx,mts,cts}",
    "chat.commandCenter.enabled": false,
    "cursor.composer.shouldChimeAfterChatFinishes": true,
    "cursor.cpp.enablePartialAccepts": true,
    "cursor.general.gitGraphIndexing": "enabled",
    "diffEditor.experimental.showMoves": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.renderSideBySide": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.accessibilitySupport": "off",
    "editor.codeActionsOnSave": {
        "quickfix.biome": "always",
        "source.organizeImports": "never"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": true,
    "editor.experimental.asyncTokenization": true,
    "editor.fontFamily": "Hack nerd font, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 11.5,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.guides.bracketPairs": true,
    "editor.indentSize": "tabSize",
    "editor.inlineSuggest.enabled": true,
    "editor.insertSpaces": true,
    "editor.letterSpacing": 0.75,
    "editor.lineHeight": 1.65,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.renderWhitespace": "trailing",
    "editor.smoothScrolling": false,
    "editor.snippetSuggestions": "top",
    "editor.suggestSelection": "first",
    "editor.wordBasedSuggestions": "currentDocument",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.sortOrder": "foldersNestsFiles",
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.mjs": "javascript"
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/": true,
        "**/node_modules/**": true,
        "**/tmp/**": true,
        "/.cache/": true,
        "/.next/": true
    },
    "githubPullRequests.pullBranch": "never",
    "playwright.reuseBrowser": false,
    "playwright.showTrace": false,
    "redhat.telemetry.enabled": false,
    "rust-analyzer.procMacro.enable": true,
    "search.exclude": {
        "**/coverage": true,
        "**/dist": true,
        "**/node_modules": true
    },
    "security.promptForLocalFileProtocolHandling": false,
    "security.workspace.trust.startupPrompt": "always",
    "typescript.disableAutomaticTypeAcquisition": true,
    "typescript.tsserver.experimental.enableProjectDiagnostics": false,
    "typescript.tsserver.log": "off",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.experimental.useGpuAcceleration": true,
    "window.zoomLevel": -0.4,
    "workbench.activityBar.location": "top",
    "workbench.colorTheme": "Lotus Dark",
    "workbench.editor.enablePreview": false,
    "workbench.editor.experimentalAutoLayout": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.statusBar.visible": false,
    "workbench.navigationControl.enabled": false,
    "window.commandCenter": false,
    "window.customTitleBarVisibility": "never",
    "material-icon-theme.folders.theme": "specific",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "explicit",
    "workbench.tree.indent": 8,
    "workbench.layoutControl.enabled": false,
    "explorer.confirmPasteNative": false,
}